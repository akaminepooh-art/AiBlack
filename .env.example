# サーバー設定
NODE_ENV=development
PORT=3001
HOST=localhost

# Python設定
PYTHON_PATH=python
PYTHON_INDICATORS_DIR=./python-indicators
PYTHON_TIMEOUT=30000

# Yahoo Finance 設定
YAHOO_FINANCE_TIMEOUT=10000
YAHOO_FINANCE_RETRY=3

# キャッシュ設定
CACHE_ENABLED=true
CACHE_TTL=300

# レート制限
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX_REQUESTS=100

# ログ設定
LOG_LEVEL=info
LOG_FILE_PATH=./logs
